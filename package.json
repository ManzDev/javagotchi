{
  "name": "parcel-template-blank",
  "version": "1.0.0",
  "description": "Blank template project starter-kit with Parcel Bundler",
  "main": "index.js",
  "scripts": {
    "dev": "parcel serve src/index.html --global global --open",
    "watch": "parcel watch src/index.html",
    "build": "parcel build src/index.html -d build --global global --public-url /",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "pug",
    "css3",
    "postcss",
    "stylelint",
    "typescript",
    "tslint",
    "javascript",
    "eslint"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "http-serve": "^1.0.1",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^6.0.7",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {}
}
