{
  "name": "parcel-template-blank",
  "version": "1.0.0",
  "description": "Blank template project starter-kit with Parcel Bundler",
  "main": "dist/index.html",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "watch": "parcel watch src/index.html",
    "build": "rm -rf dist/ && parcel build --public-url /javagotchi/ src/index.html",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "parcel",
    "template",
    "html5",
    "pug",
    "css3",
    "postcss",
    "stylelint",
    "typescript",
    "tslint",
    "javascript",
    "eslint"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.19.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.2.6",
    "postcss-easy-import": "^3.0.0",
    "postcss-font-magician": "^3.0.0",
    "postcss-preset-env": "^6.0.7",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^20.0.0"
  }
}
