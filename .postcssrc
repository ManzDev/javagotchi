{
  "plugins": {
    "postcss-easy-import": {
      "prefix": "_",
      "extensions": [
        ".pcss",
        ".css"
      ]
    },
    "postcss-preset-env": {
      "stage": 3,
      "features": {
        "nesting-rules": true,
        "case-insensitive-attributes": true,
        "hexadecimal-alpha-notation": true,
        "place-properties": true
      }
    },
    "postcss-font-magician": true,
    "autoprefixer": true
  }
}
